{
  "name": "convnet",
  "version": "0.0.1",
  "file": "models/convnet.pb",
  "options": {

    "datasets": {
      "fomoro/mnist": "0.0.1"
    },

    "epochs": 10000,
    "checkpoint_interval": 100,
    "batch_size": 50,

    "train_feed_dict": {
      "x": "<-x",
      "y_": "<-y",
      "keep_prob": 0.5
    },
    "validation_feed_dict": {
      "x": "<-x",
      "y_": "<-y",
      "keep_prob": 1.0
    },
    "test_feed_dict": {
      "x": "<-x",
      "y_": "<-y",
      "keep_prob": 1.0
    },

    "train_ops": { "train<-": "train_step" },
    "validation_ops": { "cost<-": "accuracy", "summary<-": "MergeSummary/MergeSummary" },
    "test_ops": { "cost<-": "accuracy" }
  }
}
